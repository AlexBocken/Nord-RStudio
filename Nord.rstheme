/* rs-theme-name: Nord*/
rs-theme-is-dark: TRUE

/* sidebar */
.ace_gutter {
  background: #2E3440;
  background-color: #2E3440;
  color: #4C566A;
}

.ace_gutter-layer {
  background: #2E3440;
  color: #4C566A;
}

/* ??? */
